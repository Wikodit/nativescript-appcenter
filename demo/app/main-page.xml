<Page xmlns="http://schemas.nativescript.org/tns.xsd" loaded="pageLoaded" class="page"
  xmlns:ui="nativescript-appcenter">
  <StackLayout class="p-20">
    <Label text="{{ message }}" class="t-20 text-center c-black" textWrap="true"/>
    <Button tap="{{ trackEvent }}" class="t-20 text-center c-black" text="track event"/>
    <Button tap="{{ testCrash }}" class="t-20 text-center c-black" text="test crash"/>
  </StackLayout>
</Page>